{
                    "compilerOptions": {
                      "target": "es6",
                      "lib": [
                        "dom",
                        "dom.iterable",
                        "esnext"
                      ],
                      "allowJs": true,
                      "skipLibCheck": true,
                      "strict": true,
                      "forceConsistentCasingInFileNames": true,
                      "noEmit": true,
                      "esModuleInterop": true,
                      "module": "esnext",
                      "moduleResolution": "node",
                      "experimentalDecorators": true,
                      "resolveJsonModule": true,
                      "isolatedModules": true,
                      "importHelpers": true,
                      "jsx": "preserve",
                      "baseUrl": "src",
                      "paths": {
                                        "*": ["src/*"]
                                        }
                    },
                    "include": [
                      "next-env.d.ts",
                      "views/**/*",
                      "**/*.ts",
                      "**/*.tsx",
                    "src/index.js"                    ],
                    "exclude": [
                      "node_modules"
                    ],
                  }